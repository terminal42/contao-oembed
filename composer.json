{
    "name": "terminal42/contao-twitter",
    "description": "Twitter integration for Contao",
    "type": "contao-bundle",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage":"http://www.terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.4",
        "contao/core-bundle":"^4.3",
        "guzzlehttp/guzzle": "5.*"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload":{
        "psr-4": {
            "Terminal42\\TwitterBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        },
        "contao-manager-plugin": "Terminal42\\TwitterBundle\\ContaoManager\\Plugin"
    }
}
