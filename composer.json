{
    "name": "terminal42/contao-oembed",
    "description": "Various oEmbed elements for Contao CMS",
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage":"http://www.terminal42.ch"
        }
    ],
    "require":{
        "php":"^7.1",
        "ext-json": "*",

        "contao/core-bundle":"^4.7",
        "doctrine/dbal": "^2.8",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "psr/log": "^1.0",
        "symfony/config": "^3.3 || ^4.0",
        "symfony/dependency-injection": "^3.3 || ^4.0",
        "symfony/http-foundation": "^3.3 || ^4.0",
        "symfony/http-kernel": "^3.3 || ^4.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload":{
        "psr-4": {
            "Terminal42\\OEmbedBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        },
        "contao-manager-plugin": "Terminal42\\OEmbedBundle\\ContaoManager\\Plugin"
    }
}
